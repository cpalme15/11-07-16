
import java.util.*;
public class Classwork {
private static Scanner k=new Scanner(System.in);
	public static void main(String[] args) {
		double a=0.0;
		int s1=0,s2=0,s3=0;
		// four types of methods
		//Void methods (args)
		//void method()
		//type method(args)
		//type method ()
		Classwork.display("Hello");
		Classwork.display(Double.toString(a));
		a=Classwork.AVG(10, 11, 12);
		s1=Classwork.readdata();
		s2=Classwork.readdata();
		s3=Classwork.readdata();
	}
	public static void greeting()
	{
		System.out.println("The end");
	}
   public static void display(String n)
   {
	   System.out.println(n);
	   
   }
   public static double AVG( int s1, int s2, int s3)
   {
	   double avg=0.0;
	   avg=(s1+s2+s3)/3.0;
	   return avg;
   }
   public static void displaystars()
   {
	   System.out.println("*********");
   }
   public static int readdata()
   
   {
	  
	   int score=k.nextInt();
	   return score;
   }
   }
